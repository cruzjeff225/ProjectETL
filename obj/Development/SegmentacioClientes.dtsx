                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    c.CustomerKey,
    c.FirstName,
    c.LastName,
    c.Gender,
    c.Education,
    c.Occupation,
    c.HouseOwnerFlag,
    c.YearlyIncome,
    c.NumberCarsOwned,
    c.TotalChildren,
    c.NumberChildrenAtHome,
    c.BirthDate,
    c.MaritalStatus,
    c.CustomerType,
    COUNT(s.SalesOrderNumber) AS TotalCompras,
    SUM(s.SalesAmount) AS TotalGastado,
    AVG(s.SalesAmount) AS PromedioCompra
FROM DimCustomer c
LEFT JOIN FactOnlineSales s ON c.CustomerKey = s.CustomerKey
GROUP BY 
    c.CustomerKey, c.FirstName, c.LastName, c.Gender, c.Education, 
    c.Occupation, c.HouseOwnerFlag, c.YearlyIncome, c.NumberCarsOwned,
    c.TotalChildren, c.NumberChildrenAtHome, c.BirthDate, c.MaritalStatus,
    c.CustomerType;</property>
